<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <!-- android:name=".MyApplication" 해야 시작할때 MyApplication 변수값 값 가짐(시작 전에 잠시 시작함) -->
    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Ai052301"
        tools:targetApi="31">
        <activity
            android:name=".InfoActivity"
            android:exported="false" /> <!-- 자동으로 만들어줌 -->
        <activity
            android:name=".MainActivity"
            android:exported="true"> <!-- 시작할 때 어떤액티비티인가 -->
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
app/manifests/AndroidMainfest.xml 경로
